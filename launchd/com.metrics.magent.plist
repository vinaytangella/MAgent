<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>  
        <key>Label</key>
            <string>com.metrics.magent</string>
        <key>ProgramArguments</key>
            <array>
                <string>/usr/bin/python3</string>
                <string>/usr/local/magent_metrics/mac_metrics_agent.py</string>
            </array>

        <key>RunAtLoad</key>
        <true/>

        <key>KeepAlive</key>
        
        <key>StandardOutPath</key>
        <string>/usr/local/magent_metrics/mac_metrics.log</string>
        
        <key>StandardErrorPath</key>
        <string>/usr/local/magent_metrics/mac_metrics_error.log</string>
    </dict>
</plist>